{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "component/Search.tsx",
    "component/GroupListings.jsx",
    "component/GroupListings.jsx",
    "app/(tabs)/Home.jsx",
    "app/listing/[id].jsx"
  ]
}
